{
	"name": "@matthemsteger/utils-fn-numbers",
	"version": "1.0.0",
	"description": "Functional utilities for numbers",
	"main": "dist/index.js",
	"module": "esm/index.js",
	"esnext": "src/index.js",
	"scripts": {
		"test": "mocha -r babel-register --recursive \"test/**/*.spec.js\"",
		"test:watch": "mocha -r babel-register --recursive --watch \"test/**/*.spec.js\"",
		"test:coverage": "nyc npm run test",
		"lint": "eslint src test && echo Lint passed.",
		"build:esm": "cross-env BABEL_ENV=esm babel src -d esm",
		"build:cjs": "cross-env BABEL_ENV=cjs babel src -d dist",
		"build": "npm-run-all build:*"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/matthemsteger/utils-fn.git"
	},
	"keywords": [
		"functional",
		"numbers"
	],
	"author": "Matt Hemsteger",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/matthemsteger/utils-fn/issues"
	},
	"homepage": "https://github.com/matthemsteger/utils-fn#readme",
	"publishConfig": {
		"access": "public"
	},
	"devDependencies": {
		"babel-cli": "^6.26.0",
		"babel-env": "^2.4.1",
		"babel-eslint": "^8.2.5",
		"babel-plugin-transform-object-rest-spread": "^6.26.0",
		"babel-register": "^6.26.0",
		"chai": "^4.1.2",
		"cross-env": "^5.2.0",
		"eslint": "^4.19.1",
		"eslint-config-airbnb-base": "^13.0.0",
		"eslint-config-prettier": "^2.9.0",
		"eslint-plugin-import": "^2.13.0",
		"eslint-plugin-prettier": "^2.6.1",
		"mocha": "^5.2.0",
		"npm-run-all": "^4.1.3",
		"nyc": "^12.0.2",
		"prettier": "^1.13.7",
		"sinon": "^6.1.0",
		"sinon-chai": "^3.2.0"
	},
	"dependencies": {
		"ramda": "^0.25.0"
	}
}
